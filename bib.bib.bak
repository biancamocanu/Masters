@article {Jain170373,
	author = {Jain, Miten and Olsen, Hugh E. and Turner, Daniel J. and Stoddart, David and Bulazel, Kira V. and Paten, Benedict and Haussler, David and Willard, Huntington F and Akeson, Mark and Miga, Karen H.},
	title = {Linear Assembly of a Human Y Centromere},
	year = {2018},
	doi = {10.1101/170373},
	publisher = {Cold Spring Harbor Laboratory},
	abstract = {The human genome reference sequence remains incomplete due to the challenge of assembling long tracts of near-identical tandem repeats in centromeric regions. To address this, we have implemented a nanopore sequencing strategy to generate high quality reads that span hundreds of kilobases of highly repetitive DNAs. Here, we use this advance to produce a sequence assembly and characterization of the centromeric region of a human Y chromosome.},
	URL = {https://www.biorxiv.org/content/early/2018/02/05/170373},
